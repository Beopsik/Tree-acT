{
  "name": "@treeact/extension",
  "description": "Tree-acT extension logic",
  "version": "0.0.1",
  "publisher": "caucse-hercules",
  "engines": {
    "vscode": "^1.47.0"
  },
  "activationEvents": [
    "onView:treeAct"
  ],
  "main": "../out/extension",
  "contributes": {
    "commands": [
      {
        "command": "treeAct.start",
        "title": "Start new Tree-acT session",
        "category": "Tree-acT"
      },
      {
        "command": "treeAct.postJson",
        "title": "Post JSON to Tree-acT Webview",
        "category": "Tree-acT"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "component-explorer",
          "title": "Component Explorer",
          "icon": "media/dep.svg"
        }
      ]
    },
    "views": {
      "component-explorer": [
        {
          "id": "treeAct",
          "name": "Tree-acT"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/node": "^15.0.0",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0"
  },
  "license": "MIT",
  "dependencies": {
    "cross-env": "^7.0.3",
    "async-wait-until": "^2.0.5"
  }
}
