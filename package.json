{
  "name": "tree-act",
  "version": "0.0.1",
  "description": "React code generator from Tree",
  "main": "out/extension",
  "repository": "https://github.com/caucse-hercules/Tree-acT.git",
  "author": "DoLim <limdongyoung0@gmail.com>",
  "publisher": "caucse-hercules",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "view",
    "extension"
  ],
  "engines": {
    "vscode": "^1.47.0"
  },
  "activationEvents": [
    "onView:treeAct"
  ],
  "contributes": {
    "commands": [
      {
        "command": "treeAct.start",
        "title": "Start new Tree-acT session",
        "category": "Tree-acT"
      },
      {
        "command": "treeAct.postJson",
        "title": "Post JSON to Tree-acT Webview",
        "category": "Tree-acT"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "component-explorer",
          "title": "Component Explorer",
          "icon": "./extension/media/dep.svg"
        }
      ]
    },
    "views": {
      "component-explorer": [
        {
          "id": "treeAct",
          "name": "Tree-acT"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm-run-all -s compile:*",
    "compile:view": "yarn workspace @treeact/view compile",
    "compile:extension": "yarn workspace @treeact/extension compile",
    "develop": "npm-run-all -s develop:*",
    "develop:view": "yarn workspace @treeact/view develop",
    "develop:extension": "yarn workspace @treeact/extension develop"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
    "@types/shelljs": "^0.8.8",
    "@visx/visx": "^1.10.0",
    "async-wait-until": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-move": "^6.4.0",
    "react-scripts": "^4.0.3",
    "react-spring": "^9.1.2",
    "shelljs": "^0.8.4"
  }
}
