{
  "name": "tree-act",
  "version": "0.0.1",
  "description": "React code generator from Tree",
  "main": "out/extension",
  "repository": "https://github.com/caucse-hercules/Tree-acT.git",
  "author": "DoLim <limdongyoung0@gmail.com>",
  "publisher": "caucse-hercules",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "treeAct-view",
    "treeAct-extension"
  ],
  "engines": {
    "vscode": "^1.47.0"
  },
  "activationEvents": [
    "onView:treeAct",
    "onWebviewPanel:treeAct",
    "onCommand:treeAct.start"
  ],
  "contributes": {
    "commands": [
      {
        "command": "treeAct.start",
        "title": "Open Tree-acT Panel",
        "category": "Tree-acT",
        "icon": {
          "light": "./treeAct-extension/resources/light/open.svg",
          "dark": "./treeAct-extension/resources/dark/open.svg"
        }
      },
      {
        "command": "treeAct.addNode",
        "title": "Add Node(component) to Tree",
        "category": "Tree-acT",
        "icon": {
          "light": "./treeAct-extension/resources/light/add.svg",
          "dark": "./treeAct-extension/resources/dark/add.svg"
        }
      },
      {
        "command": "treeAct.deleteNode",
        "title": "Delete Node(component) from Tree",
        "category": "Tree-acT"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "treeAct",
          "title": "Tree-acT",
          "icon": "./treeAct-extension/media/treeAct.svg"
        }
      ]
    },
    "views": {
      "treeAct": [
        {
          "id": "treeAct",
          "name": "Tree-acT"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "treeAct.start",
          "when": "view == treeAct",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm-run-all -s compile:*",
    "compile:view": "yarn workspace @treeact/view compile",
    "compile:extension": "yarn workspace @treeact/extension compile",
    "develop": "npm-run-all -s develop:*",
    "develop:view": "yarn workspace @treeact/view develop",
    "develop:extension": "yarn workspace @treeact/extension develop"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "yarn": "^1.22.10"
  }
}
