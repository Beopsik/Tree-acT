{
  "name": "treeAct",
  "description": "Tree to React Code Generator",
  "version": "0.0.1",
  "publisher": "caucse-hercules",
  "engines": {
    "vscode": "^1.47.0"
  },
  "activationEvents": [
    "onView:treeAct"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "treeAct.start",
        "title": "Start new Tree-acT session",
        "category": "Tree-acT"
      },
      {
        "command": "treeAct.postJson",
        "title": "Post JSON to Tree-acT Webview",
        "category": "Tree-acT"
      },
      {
        "command": "treeAct.generateCode",
        "title": "Generate code from sample data",
        "category": "Tree-acT"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "component-explorer",
          "title": "Component Explorer",
          "icon": "media/dep.svg"
        }
      ]
    },
    "views": {
      "component-explorer": [
        {
          "id": "treeAct",
          "name": "Tree-acT"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm-run-all -s compile:*",
    "compile:webpack": "webpack",
    "compile:extension": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "npm-run-all -s watch:*",
    "watch:webpack": "webpack",
    "watch:extension": "tsc -w -p ./",
    "start": "webpack serve"
  },
  "devDependencies": {
    "@types/node": "^15.0.0",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/vscode": "^1.55.0",
    "@types/webpack-dev-server": "^3.11.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "html-webpack-plugin": "^5.3.1",
    "react-dev-utils": "^11.0.4",
    "source-map-loader": "^2.0.1",
    "ts-loader": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.36.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "license": "MIT",
  "dependencies": {
    "@types/shelljs": "^0.8.8",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "run-in-terminal": "^0.0.3",
    "shelljs": "^0.8.4"
  }
}